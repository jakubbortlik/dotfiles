    </keybind>
    <keybind key="W-d">               # toggle decoration in the current window
      <action name="ToggleDecorations"/>
    </keybind>
    <keybind key="W-S-d">             # toggle decoration in all windows
      <action name="ForEach"><then><action name="ToggleDecorations"/></then></action>
    </keybind>
